<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="670.0" prefWidth="1175.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxsspger.controladores.FXMLAnteproyectoController">
   <children>
      <Pane prefHeight="66.0" prefWidth="1175.0" style="-fx-background-color: #00529E;">
         <children>
            <ImageView fitHeight="45.0" fitWidth="47.0" layoutX="14.0" layoutY="11.0" onMouseClicked="#clicCerrarVentana" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../recursos/arrow.png" />
               </image>
            </ImageView>
            <Label fx:id="lbTitulo" layoutX="89.0" layoutY="17.0" text="Anteproyectos Publicados" textFill="WHITE">
               <font>
                  <Font name="Roboto Regular" size="25.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <TableView fx:id="tvAnteproyectos" layoutX="28.0" layoutY="160.0" prefHeight="487.0" prefWidth="1121.0">
         <columns>
            <TableColumn fx:id="colNombre" prefWidth="541.0" resizable="false" text="Nombre" />
            <TableColumn fx:id="colDirector" prefWidth="173.0" text="Director" />
            <TableColumn fx:id="colAlumnosMaximos" prefWidth="72.0" text="Alum. Máx." />
            <TableColumn fx:id="colDuracion" prefWidth="99.0" text="Duración Aprox." />
            <TableColumn fx:id="colModalidad" prefWidth="150.0" text="Modalidad" />
            <TableColumn fx:id="colEstado" prefWidth="85.0" text="Estado" />
         </columns>
      </TableView>
      <TextField fx:id="tfBusqueda" layoutX="28.0" layoutY="97.0" prefHeight="27.0" prefWidth="324.0" promptText="Buscar Anteproyecto..." />
      <Button fx:id="btnVerPostulados" layoutX="573.0" layoutY="98.0" mnemonicParsing="false" onAction="#clicVerAnteproyectosPostulados" style="-fx-background-color: #00529E;" text="Ver Anteproyectos Postulados" textFill="WHITE">
         <font>
            <Font name="Microsoft JhengHei Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnVerPublicados" disable="true" layoutX="789.0" layoutY="98.0" mnemonicParsing="false" onAction="#clicVerAnteproyectosPublicados" style="-fx-background-color: #00529E;" text="Ver Anteproyectos Publicados" textFill="WHITE">
         <font>
            <Font name="Microsoft JhengHei Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnVerMisAnteproyectos" layoutX="1000.0" layoutY="98.0" mnemonicParsing="false" onAction="#clicVerMisAnteproyectos" style="-fx-background-color: #00529E;" text="Ver Mis Anteproyectos" textFill="WHITE">
         <font>
            <Font name="Microsoft JhengHei Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnCrearAnteproyecto" disable="true" layoutX="28.0" layoutY="98.0" mnemonicParsing="false" onAction="#clicCrearAnteproyecto" style="-fx-background-color: green;" text="Crear Anteproyecto" textFill="WHITE" visible="false">
         <font>
            <Font name="Microsoft JhengHei Bold" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnModificarAnteproyecto" layoutX="197.0" layoutY="98.0" mnemonicParsing="false" onAction="#clicModificarAnteproyecto" style="-fx-background-color: yellow;" text="Modificar Anteproyecto" visible="false">
         <font>
            <Font name="Microsoft JhengHei Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
